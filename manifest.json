{
  "manifest_version": 3,
  "name": "Timestamp converter",
  "description": "Timestamp converter",
  "version": "1.0.0",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "minimum_chrome_version": "102",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"]
    }
  ]
}
